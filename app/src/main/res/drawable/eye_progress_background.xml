<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/gray" />
            <corners
                android:bottomLeftRadius="@dimen/progressbar_h"
                android:bottomRightRadius="@dimen/progressbar_h"
                android:topLeftRadius="@dimen/progressbar_h"
                android:topRightRadius="@dimen/progressbar_h" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle">
                <solid android:color="@color/black" />
                <corners
                    android:bottomLeftRadius="@dimen/progressbar_h"
                    android:bottomRightRadius="@dimen/progressbar_h"
                    android:topLeftRadius="@dimen/progressbar_h"
                    android:topRightRadius="@dimen/progressbar_h" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <solid android:color="@color/eye_progress_f" />
                <corners
                    android:bottomLeftRadius="@dimen/progressbar_h"
                    android:bottomRightRadius="@dimen/progressbar_h"
                    android:topLeftRadius="@dimen/progressbar_h"
                    android:topRightRadius="@dimen/progressbar_h" />
            </shape>
        </clip>
    </item>
</layer-list>